@prefix skos: <http://www.w3.org/2004/02/skos/core#> .


<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/TITEL> skos:prefLabel "Titel" ;
	skos:definition "Benennung einer Verzeichnungseinheit" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/SIGNATUR> skos:prefLabel "Signatur" ;
	skos:definition "Feld für konkatenierbares Signatur-Element zur Identifizierung und Anzeige von Verzeichnungseinheiten" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/GEBURTSDATUM> skos:prefLabel "Geburtsdatum" ;
	skos:scopeNote "Einzeldatum" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/TODESDATUM> skos:prefLabel "Todesdatum" ;
	skos:scopeNote "Einzeldatum" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/STUFE> skos:prefLabel "Verzeichnungsstufe" ;
	skos:definition "Zuordnung einer VE zu einer Verzeichnungsstufe" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ENTSTEHUNGSZEITRAUM> skos:prefLabel "Entstehungszeitraum" ;
	skos:definition "Identifizierung der  Laufzeit bzw. des Entstehungsdatums  oder -zeitraums der in  einer Verzeichnungseinheit enthaltenen Unterlagen" ;
	skos:scopeNote "Datumsbereich (von/bis)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/DARIN> skos:prefLabel "Darin" ;
	skos:definition "kann verwendet werden für die Eingabe von Detail-Informationen, welche (innerhalb der erfassten VE) vor anderen hervorgehoben zu werden verdienen resp. aufgrund des Titels nicht in dieser VE vermutet werden." ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/BILD_ANSICHT> skos:prefLabel "Bild Ansicht" ;
	skos:scopeNote "Bild" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/FINDHILFSMITTEL> skos:prefLabel "Findhilfsmittel" ;
	skos:definition "Identifizierung aller für die Verzeichnungseinheit vorhandenen Findmittel" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/VE_ZUGANG_LINK> skos:prefLabel "Zugänge" ;
	skos:definition "Zugang zu einem Bestand respektive Verzeichnungseinheit. Hier kann eine Zugangsnummer eingegeben sowie eine Ablieferung ausgewählt werden. Es handelt sich um eine Verknüpfung auf das Ablieferungsmodul. Im Ablieferungsmodul ist der Zugang im Register \"Verzeichnungseinheiten\" sichtbar. Intern heisst der Verknüpfungstyp \"Zugang\"" ;
	skos:scopeNote "Zugänge" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ERSCHLIESSUNGSGRAD> skos:prefLabel "Erschliessungsgrad" ;
	skos:definition "Der Erschliessungsgrad einer Verzeichnungseinheit. Dieser Wert ist auch in der Listensicht des Modulsverzeichnungseinheiten sichtbar." ;
	skos:scopeNote "Auswahlliste" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ARCHIVALIENART> skos:prefLabel "Archivalienart" ;
	skos:scopeNote "Auswahlliste" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/FRÜHERE_STABS_SIGNATUR> skos:prefLabel "Frühere StABS-Signatur" ;
	skos:definition "Signaturen, die früher in Gebrauch waren. Signaturen können bei einer Mutation automatisch in dieses Feld übertragen werden, sofern die entsprechende Systemoption aktiviert wurde." ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ABSPIELDAUER> skos:prefLabel "Abspieldauer" ;
	skos:definition "Spieldauer eine Audioaufzeichnung. Dieser Wert kann im Modul Verzeichnungseinheiten auf einer übergeordneten Stufe kumuliert werden." ;
	skos:scopeNote "Zeitdauer" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ANZAHL> skos:prefLabel "Anzahl" ;
	skos:definition "Die Anzahl Einheiten oder Behältnisse einer Verzeichnungseinheit (gehört zur Umfangangabe). Dieser Wert kann im Modul Verzeichnungseinheiten auf einer übergeordneten Stufe kumuliert werden." ;
	skos:scopeNote "Ganze Zahl" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/LAUFMETER> skos:prefLabel "Laufmeter" ;
	skos:definition "Die Laufmeter einer Verzeichnungseinheit. Dieser Wert kann im Modul Verzeichnungseinheiten auf einer übergeordneten Stufe kumuliert werden." ;
	skos:scopeNote "Festkomma-Zahl" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ENTHAELT> skos:prefLabel "Enthält" ;
	skos:definition "kann verwendet werden für die Beschreibung des Inhalts einer VE." ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/VE_AKTENBILDNER_LINK> skos:prefLabel "Aktenbildner/Provenienz (Link)" ;
	skos:definition "Die aktenbildenden Organisation respektive Person, welche die Verzeichnungseinheit ursprünglich produziert hat.  Hier handelt es sich um eine Verknüpfung auf einen Partner, die im Formular einer Verzeichnungseinheit erfasst wird. Im Partnermodul ist die Verbindung im Verknüpfungsregister sichtbar (Verknüpfungstyp \"Aktenbildner der Verz.-Einheit\"). Andersprachige Bezeichnungen: Creator (E); Producteur (F);  Soggetto produttore (I)." ;
	skos:scopeNote "Verknüpfung" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/VE_ABLIEFERUNG_LINK> skos:prefLabel "Ablieferung (Link)" ;
	skos:definition "Die Ablieferung, aus der eine Verzeichnungseinheit ursprünglich stammt. Dieses Datenelement wird im Formular einer Verzeichnungseinheit erfasst und ist ein Link auf eine Ablieferung. Im Modul Ablieferungen ist die Verknüpfung im Register \"Verz.-Einheiten\" sichtbar. Der interne Verknüpfungstyp heisst \"Ablieferung der Verz.-Einheit\"." ;
	skos:scopeNote "Verknüpfung" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/AKTENBILDNER_PROVENIENZ_TEXT> skos:prefLabel "Aktenbildner/Provenienz (Text)" ;
	skos:definition "Migriert von scopeArchiv 2.3" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<IDENTIFIKATION::> skos:prefLabel "Identifikation:" ;
	skos:definition "Identifikationsbereich" ;
	skos:scopeNote "Zwischentitel" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/VERWEISE> skos:prefLabel "Verweise" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/INHALT> skos:prefLabel "Inhalt" ;
	skos:definition "kurze  Zusammenfassung des wesentlichen Inhalts (einschliersslich Zeitraum) der beschriebenen Verzeichnungseinheit" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<KONTEXT::> skos:prefLabel "Kontext:" ;
	skos:definition "Bereich für die Beschreibung des Entstehungskontextes von Verzeichnungseinheiten" ;
	skos:scopeNote "Zwischentitel" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ORT> skos:prefLabel "Ort" ;
	skos:definition "entspricht MAD II 22.4C1: \"Site or place\"" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/UMFANG> skos:prefLabel "Umfang" ;
	skos:definition "quantitative Angabe zu einer  Verzeichnungseinheit" ;
	skos:scopeNote "Zwischentitel" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/BEHÄLTNISSE> skos:prefLabel "Behältnisse" ;
	skos:definition "Anzahl von Behältnissen einer Verzeichnungseinheit" ;
	skos:scopeNote "Ganze Zahl" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/BEHÄLTNISTYP_(HÖFLINGER)> skos:prefLabel "Behältnistyp (Höflinger)" ;
	skos:definition "Zuordnung eines gezählten Behältnisses zu einem Behältnis-Typ" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/BIBLIOTHEKSKATALOG> skos:prefLabel "Bibliothekskatalog" ;
	skos:definition "Referenz auf Bibliotheks-Signatur einer Veröffentlichung in der Bibliothek des StABS oder (evt.) einer anderen über das gleiche System zugänglichen Fachbibliothek (UBB usw.)." ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/VERWALTUNGSGESCHICHTE_BIOGRAFISCHE_ANGABEN> skos:prefLabel "Verwaltungsgeschichte/Biografische Angaben" ;
	skos:definition "Geschichte einer Provenienzstelle von Verzeichnungseinheiten" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ZEITRAUM_DER_MATERIALZUSAMMENSTELLUNG> skos:prefLabel "Zeitraum der Materialzusammenstellung" ;
	skos:definition "Datierung der durchz die Provenienzstelle erfolgten Materialzusammenstelloung" ;
	skos:scopeNote "Datumsbereich (von/bis)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/BESTANDSGESCHICHTE> skos:prefLabel "Bestandsgeschichte" ;
	skos:definition "Information über Wechsel der Eigentums- und Besitzverhältnisse von Verzeichnungseinheiten vor deren Übergang an die für verzeichnete (letzte) Provenienzstelle sowie weitere Angaben über die Geschichte dieser Verzeichnugnseinheiten, welche fürRückschlüsse auf Authentizität, Vollständigkeit und Interpretation derselben wesentlich sind." ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/INHALT_UND_INNERE_ORDNUNG%3A> skos:prefLabel "Inhalt und innere Ordnung:" ;
	skos:scopeNote "Zwischentitel" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/NEUZUGÄNGE> skos:prefLabel "Neuzugänge" ;
	skos:definition "Angaben für die/den BenützerIn über mögliche Veränderungen im Umfang der  Verzeichnungseinheit " ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ZUGANGS__UND_BENUTZUNGSBEDINGUNGEN%3A> skos:prefLabel "Zugangs- und Benutzungsbedingungen:" ;
	skos:scopeNote "Zwischentitel" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/RECHTSSTATUS> skos:prefLabel "Rechtsstatus" ;
	skos:definition "Information über den Rechtsstatus  der Verzeichnungseinheit" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ZUGANGSBESTIMMUNGEN> skos:prefLabel "Zugangsbestimmungen" ;
	skos:definition "Angabe derjenigen Bestimmungen, die den Zugang zur  Verzeichnungseinheit einschränken oder beeinflussen" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/COPYRIGHT_REPORDUKTIONSBESTIMMUNGEN> skos:prefLabel "Copyright/Reproduktionsbestimmungen" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/SPRACHE> skos:prefLabel "Sprache" ;
	skos:definition "Identifizierung der in der Verzeichnungseinheit enthaltenen Sprache(n), Schriftarten und Zeichensysteme" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/SACHVERWANDTE_UNTERLAGEN> skos:prefLabel "Sachverwandte Unterlagen:" ;
	skos:scopeNote "Zwischentitel" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/AUFBEWAHRUNGSORT_DER_ORIGINALE> skos:prefLabel "Aufbewahrungsort der Originale" ;
	skos:definition "Ermittlung des Archivs, der Körperschaft oder der Person, in deren Besitz die Vorlage ist, dalls es sich  bei  der beschriebenen Verzeichnungseinheit um eine Reproduktion handelt." ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/KOPIEN_BZW._REPRODUKTIONEN> skos:prefLabel "Kopien/Reproduktionen" ;
	skos:definition "Verweis  auf Kopien bzw. reproduktionen  der Verzeichnungseinheit und ihre Verfügbarkeit" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/VERWANDTE_VE_NICHT_IN_SCOPEARCHIV> skos:prefLabel "Verwandte VE nicht in scopeArchiv" ;
	skos:definition "Ermittlung verwandter Verzeichnungseinheiten im StABS, die nicht in socpeArchiv erfasst sind." ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/VERWANDTES_MATERIAL> skos:prefLabel "Verwandtes Material" ;
	skos:definition "Verweis auf Unterlagen  in anderen Archiven, die durch ihre Provenienz in verbindung zur Verzeichnungseinheit stehen" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ANMERKUNGEN> skos:prefLabel "Anmerkungen" ;
	skos:definition "Spezialinformationen und  Angaben zu einer Verzeichnungseinheit, welche in keinen anderen der definierten ISAD(G)-Beschreibungselemente angebracht werden können." ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/MASSSTAB> skos:prefLabel "Massstab" ;
	skos:definition "Angabe der massstäblichen Vergrösserung oder Verkleinerung. " ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/VERFERTIGERIN> skos:prefLabel "VerfertigerIn" ;
	skos:definition "entspricht Rubrik im Eingangsbuch Planarchiv" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/AUSSTELLUNGSDATUM> skos:prefLabel "Ausstellungsdatum" ;
	skos:definition "Tag, an welchem eine Handlung (Vertrag  usw.) urkundlich beglaubigt wurde " ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/UNTERZEICHNERIN> skos:prefLabel "UnterzeichnerIn" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/TECHNIK> skos:prefLabel "Technik" ;
	skos:definition "zur Beschreibung von Fotonegativen, Bildern oder Plänen" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ORIGINALTITEL_AUF_HÜLLE> skos:prefLabel "Originaltitel auf Hülle" ;
	skos:definition "(wird verwendet zur Beschreibung von Fotonegativen des Fotoarchivs Höflinger)" ;
	skos:scopeNote "Ja/Nein-Feld" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/POSITIV_IN_SCHACHTEL> skos:prefLabel "Positiv in Schachtel" ;
	skos:definition "Vorhandenes Positiv zum erfassten Negativ" ;
	skos:scopeNote "Auswahlliste" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/AKTEN_ZUM_AUFTRAG> skos:prefLabel "Akten zum Auftrag" ;
	skos:definition "Verweis auf vorhandene Akten" ;
	skos:scopeNote "Ja/Nein-Feld" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/REPRODUKTION> skos:prefLabel "Reproduktion" ;
	skos:definition "Angabe, ob die erfasste Verzeichnungseinheit eine Reproduktion ist" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ALTE_SIGNATUREN> skos:prefLabel "Alte Signaturen" ;
	skos:definition "wird verwendet zur Angabe allfällig vorhandener alter Signaturen." ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/FORMAT_DER_UNTERLAGE> skos:prefLabel "Format der Unterlage" ;
