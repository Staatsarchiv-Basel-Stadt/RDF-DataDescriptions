	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/VERFERTIGER_ERSCHLOSSEN> skos:prefLabel "Verfertiger erschlossen" ;
	skos:definition "Das B-Element wird benötigt zur Unterscheidung von Autorschafts-Angaben, welche dem Material direkt entnoimmen werden können, und solchen, die indirekt erschlossen werden müssen." ;
	skos:scopeNote "Ja/Nein-Feld" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ORIGINALBEZEICHNUNG> skos:prefLabel "Originalbezeichnung" ;
	skos:definition "Auf der Verzeichnungseinheit enthaltene Bezeichnung oder Titelgebung, welche nicht mit dem erfassten Titel übereinstimmt" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/FORMAT_BILD_PLAN> skos:prefLabel "Format Bild/Plan" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ANMERKUNGEN%3A> skos:prefLabel "Anmerkungen:" ;
	skos:scopeNote "Zwischentitel" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/VERTRAGSPARTNER> skos:prefLabel "Vertragspartner " ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/VERTRAGSGEGENSTAND> skos:prefLabel "Vertragsgegenstand" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ARCHIVSIGEL> skos:prefLabel "Archivsigel" ;
	skos:definition "Akronym der  Archiv-Organisation " ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/WIRKUNGSFELD> skos:prefLabel "Tätigkeiten, öffentliche Rolle" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/UMFANG_GESCHÄTZT> skos:prefLabel "Umfang approximativ" ;
	skos:definition "Feld zur fakultativen Eingabe eines Wertes für den Umfang, wenn keine präzisen Zahlen verfügbar sind" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ENTSTEHUNGSZEIT_SCHWERGEWICHT_> skos:prefLabel "Entstehungszeitraum Schwergewicht " ;
	skos:definition "Eckdaten der meisten in einer Verzeichnungseinheit enthaltenen   Unterlagen" ;
	skos:scopeNote "Datumsbereich (von/bis)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/BEWERTUNG_UND_KASSATION> skos:prefLabel "Bewertung und Kassation" ;
	skos:definition "Bereitstellung von Informationen über jede an der Verzeichnungseinheit vorgenommemnen Bewertung und Kassation" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/VERÖFFENTLICHUNGEN> skos:prefLabel "Veröffentlichungen" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/PRIORITÄTSKLASSE> skos:prefLabel "Prioritätsklasse" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/DIREKTÜBERNAHME_VON_PROVENIENZSTELLE> skos:prefLabel "Direktübernahme von Provenienzstelle" ;
	skos:definition "Darstellung der Umstände, die mit der direkten Übernahme der Verzeichnungseinheit von der Provenienzstelle verbunden sind." ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/FORM_UND_INHALT> skos:prefLabel "Form und Inhalt" ;
	skos:definition "Festellung von Hauptgegenstand und Form der Verzeichnungseinheit, um BenützerInnen eine Beurteilung ihrer Relevanz zu ermöglichen." ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ORDNUNG_UND_KLASSIFIKATION> skos:prefLabel "Ordnung und Klassifikation" ;
	skos:definition "Information über die Ordnung und Klassifikation der Verzeichnungseinheit" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ENTSTEHUNGSZEITRAUM__STREUDATEN> skos:prefLabel "Entstehungszeitraum, Streudaten" ;
	skos:definition "Wird verwendet für die Zeitangabe einzelner isolierter Teile innerhalb einer Verzeichnungseinheit" ;
	skos:scopeNote "Datumsbereich (von/bis)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/FRUEHERE_SIGNATUR> skos:prefLabel "Frühere Signatur" ;
	skos:definition "Angabe der von der Provenienzstelle verwendeten Signatur oder identifizierenden Bezeichnung" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/ARCHIVGESCHICHTE> skos:prefLabel "Archivgeschichte" ;
	skos:definition "Dient als Feld zur Präsentation der wichtigsten Informationen über die Geschichte des betreffenden Archivs" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/LITERATUR_BEST_INSG> skos:prefLabel "Literatur zum Bestand  insgesamt" ;
	skos:scopeNote "Text (max. 4000 Z.)" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/FORMAT_(NEGATIV_HÖFLINGER)> skos:prefLabel "Format (Höflinger)" ;
	skos:definition "wird verwendet zur Signaturenbildung im Fotoarchiv Höflinger (Werte: \"A\", \"H\", \"P\")" ;
	skos:scopeNote "Auswahlliste" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/NICHT_IDENTIFIZIERT> skos:prefLabel "Nicht identifiziert" ;
	skos:definition "für Verzeichnugnsdeinheiten, deren Inhalt  nicht identifiziert werden konnte" ;
	skos:scopeNote "Ja/Nein-Feld" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/REPRO_HÖFLINGER> skos:prefLabel "Repro Höflinger" ;
	skos:scopeNote "Ja/Nein-Feld" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/AUFNAHMEDATUM> skos:prefLabel "Aufnahmedatum" ;
	skos:definition "ISAD(G): ISAAR(CPF)2004: Angabe, wann die Normdatei erstellt, überarbeitet oder gelöscht wurde." ;
	skos:scopeNote "Einzeldatum präzis" .

<http://data.staatsarchiv-bs.ch/verzeichnungseinheit/REVISIONSDATUM> skos:prefLabel "Revisionsdatum" ;
	skos:definition "ISAD(G):  ISAAR(CPF)2004: Angabe, wann die Normdatei erstellt, überarbeitet oder gelöscht wurde." ;
